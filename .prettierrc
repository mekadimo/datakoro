{
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
    "plugins": ["prettier-plugin-svelte"],
    "printWidth": 80,
    "quoteProps": "consistent",
    "semicolons": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false
}
